<Window x:Class="WpfApplication5.Tests._Frame"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:WpfApplication5.Tests"
        mc:Ignorable="d"
        Title="_Frame" >
    <StackPanel>
        <TextBlock>Window</TextBlock>  
        <Frame Source="_Window.xaml"/> 
        <TextBlock>Page</TextBlock>  
        <Frame Source="_Page.xaml"/>  
        <TextBlock>User Control</TextBlock>  
        <Frame>
            <Frame.Source>_UserControl.xaml</Frame.Source>
            <Frame.LayoutTransform>  
                <RotateTransform Angle="45" />  
            </Frame.LayoutTransform> 
        </Frame>
    </StackPanel>
</Window>
